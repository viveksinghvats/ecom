    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-socket.io": "^6.11.11",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/websockets": "^6.11.11",
    "@sendgrid/mail": "^7.1.0",
    "@sentry/node": "^5.7.1",
    "@types/algoliasearch": "^3.34.1",
    "@types/bcrypt": "^3.0.0",
    "@types/braintree": "^2.16.4",
    "@types/cron": "^1.7.2",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.5.11",
    "@types/node-geocoder": "^3.19.1",
    "@types/nodemailer": "^6.2.1",
    "@types/sharp": "^0.24.0",
    "@types/stripe": "^6.31.20",
    "@types/uuid": "^3.4.5",
    "add-subtract-date": "^1.0.14",
    "algoliasearch": "^3.34.0",
    "app-root-path": "^3.0.0",
